<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
      <add key="GraphUrl" value="https://graph.microsoft.com" />
      <add key="ClientId" value="[clientid]" />
      <add key="Thumbprint" value="[thumbprint]" />
      <add key="Authority" value="https://login.windows.net/organizer.onmicrosoft.com/" />
      <add key="apikey" value="[searchapikey]"/>
      <add key="servicename" value="[searchservicename]"/>
    </appSettings>
</configuration>